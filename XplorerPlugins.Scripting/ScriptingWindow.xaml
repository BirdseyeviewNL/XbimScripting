<UserControl
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        MinWidth="300"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        d:DesignWidth="343.497" d:DesignHeight="330.786"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"        
        xmlns:presentation="clr-namespace:XplorerPlugins.Scripting.Presentation"
        mc:Ignorable="d" x:Class="XplorerPlugins.Scripting.ScriptingWindow">
    <Grid>
        <TabControl>
            <TabItem Header="Script">
                <presentation:ScriptingControl x:Name="ScriptingControl"  Model="{Binding}"/>
            </TabItem>
            <TabItem Header="Script Results" Name="ScriptResults" />
        </TabControl>
    </Grid>
</UserControl>